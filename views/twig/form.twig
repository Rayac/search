{% extends "base.html" %}

{% block title %}Form{% endblock %}
{% block body %}
    <div class="content">
        <div class="formDiv">
            <form class="form" action="/" method="post">
                <label for="find">Find:<input class="search" type="text" name="find"><br></label>
                <input class="submitButton" type="submit" id="sendMail" value="Submit">
            </form>
        </div>
        <br><br>

        <div class="results">
            <ul>
                {% for website in websites %}
                   <li> <div class="human">

                        <img src="{{ website['ImageURL'] }}" alt="Profile pic">

                            <p><span>Name:</span> {{ website['Name'] }}<br><span>Description:</span> {{ website['Description'] }}</p>
                    </div></li>
                {% endfor %}
            </ul>
        </div>
    </div>
{% endblock %}